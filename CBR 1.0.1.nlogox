<?xml version="1.0" encoding="utf-8"?>
<model version="NetLogo 7.0.1" snapToGrid="true">
  <code><![CDATA[;; first version of the model 24 October 2025

breed [actors actor]
breed [tasks task]
breed [resources resource]

directed-link-breed [task-links task-link]
undirected-link-breed [resource-links resource-link]
undirected-link-breed [actor-links actor-link]

; an actor can be an employee, a manager, or an executive
; if needs be - a manager is defined by a color and a 
; marker; also there are departments and teams for 
; each manager and employee

actors-own [
  team
  department
  manager?
  
  sociability ;; the extent to which an agent is a social individual
  competence
  energy ;; higher efforts on a task/resource may reduce resource use 
         ;; effectiveness temporarily, at least
  energy-cap ;; the high end the energy can get for each actor
  
  counter ;; how many tasks performed
  count-ord
  count-mr
  count-he
]

tasks-own [
  worked-on?
  t-type ;; this is the type of task and it could range between
         ;; ordinary (low) to a mid-range to a high-range
  urgency ;; how much central to operations is each single task (pressure); it is the 
             ;; combination of this value with the type that makes
             ;; a task performed (and ultimately disappear from the system)
  draining   ;; how much energy is required
]

resources-own [
  ;; we can think of three types of resources: 
  ; Type A: standard (easy to use) 
  ; Type B: advanced (need competence)
  ; Type C: new tools (need curiosity and openness to change)
  r-type
  potential ;; a value for exploiting the resource (how effective 
            ;; it is depends on interactions with the actor and the task)
  usage ;; when a resource is not used, it fades away until it hides
]


to setup
  clear-all
  reset-ticks
  
  crt #actors [
    set breed actors
    set color yellow
    set shape "circle"
    set size 0.7
    setxy random-xcor random-ycor
    
    set sociability median (list (0) (random-normal 1 0.5) (2))
    ;; few are particularly good, most are average
    ;; or below
    set competence random-exponential mean_competence 
    
    set energy median (list (0) (random-normal 1 0.5) (2))
    set energy-cap energy
  ]
  ask one-of actors [
    set manager? 1
    set color red
  ]
  setup-actor-links

  task-input
  resource-input

end


to setup-actor-links
  ask actors [
    create-actor-links-with other actors in-radius reach [
      set color yellow
    ]
  ]
end

;; GENERAL GO PROCEDURE

to start
  task-move
  resource-development
  
  work
  
  if ticks = timeline [ stop ]
end


;; TASK RELATED PROCEDURES

to task-input
  ;; since tasks fall off the top of the grid,
  ;; and the grid has 32 spots, the number is 32
  crt (random 32) [
    set breed tasks
    set shape "square"
    set color [21 120 75] ; bow darker green; it was: 21 173 35social
    setxy random-xcor 16
    
    set t-type one-of (list "ordinary" "mid-range" "high-end")
    set urgency random-normal 0 1 ;; neutral, neglectable < 0 and urgent > 0
  ]
  
  ;; each task type has a corresponding energy value:
  ask tasks [
    ifelse t-type = "ordinary" [
      set draining 0.2
    ][
      ifelse t-type = "mid-range" [
        set draining 0.4
      ][
        set draining 1
      ]
    ]
  ]
  ;task-move
end

to task-move
  tick
  
  ;task-input
  
  ask tasks [
    ifelse [ycor] of self = -16 [
      setxy ([xcor] of self) 16
    ][
      ifelse [ycor] of self < 1 [
        facexy ([xcor] of self) (-16)
        fd 0.5
      ][
        facexy ([xcor] of self) (0)
        fd 0.5
      ]
    ]
  ]
  
  ;; tasks will appear in relation to the frequency
  ;; slider; when the slider is 1 then a new task appears
  ;; at every tick. One tick is one work hour; a day of 
  ;; work is 8 hours, hence, a relaxed worker would
  ;; deal with a new (difficult) task every three days
  ;; while easy tasks may appear more frequently
  ;; (but this about easy vs hard may be an add-on
  ;; to be thought through later. 3 days are 8x3=24 ticks
  ifelse task_frequency = 1 [
    task-input
  ][
    let cos-factor (ticks * (-75 * 0.2 + 15 + 75 * task_frequency))
    if (cos cos-factor) = 1 [
      task-input
    ]
  ]
end

;; RESOURCE RELATED PROCEDURES

to resource-input
  ;; at least 20 resources are created initially + some more
    crt 40 [
    set breed resources
    set shape "triangle"
    set color sky ; [102 160 185] 
    setxy random-xcor random-ycor
    
    set r-type one-of (list "Type A" "Type B" "Type C")
    set potential random-float 3
    set usage 0.1
  ]
end

to resource-development
  ask resources [
    ifelse hidden? = TRUE [
      ;; when a resource is hidden then it moves
      ;; until it may find a link/use
      fd 0.1
    ] [
      if count my-links = 0 [
        set color ([color] of self + usage)
      ]
      if [color] of self > sky + 5 [
        set hidden? TRUE
      ]
    ]
  ]
  
  ;; when someone connects to a resrouce, then
  ;; it becomes visible again
  ask resources [
    if any? my-links [
      set hidden? FALSE
      set color sky
    ]
  ]
  
  ;; new resources are made available depending
  ;; on investments; the mechanism is the same
  ;; used above for tasks
  ifelse resource_frequency = 1 [
    ask n-of (random 5) resources [
      hatch-resources 1 [
        setxy random-xcor random-ycor
      ]
    ]
  ][
    ifelse resource_frequency = 0 [
    ][
      let cos-factor (ticks * (-75 * 0.2 + 15 + 75 * resource_frequency))
      if (cos cos-factor) = 1 [
        ask n-of (random 5) resources [
          hatch-resources 1 [
            setxy random-xcor random-ycor
          ]
        ]
      ]
    ]
  ]
  
end

;; WORKING ON TASKS WITH RESOURCES

; the chooser Rationality has the following options:
;"bounded" -- classic Simonian rationality, where the 
;"ecological"
;"biased"
;"epistemic"
;"extendable"

to work
  ifelse Rationality = "bounded" [
    bounded-rationality
  ][
    ifelse Rationality = "ecological" [
      ecological-rationality
    ][
      ifelse Rationality = "biased" [
        biased-rationality
      ][
        ifelse Rationality = "epistemic" [
          epistemic-rationality
        ][
          extendable-rationality
        ]
      ]
    ]
  ]
  
  ; moved down, after BR, since most modules pass
  ; through it...
  ; performance
  
end

to bounded-rationality
  ask actors [    
    create-resource-links-with resources in-radius reach [
      set color sky
    ]
    ;; there needs to be a cap for how many tasks one can handle
    ;; the cap is provided by a variable linked to energy -- energy is max 2.
    ;; The cap is roughly 0.2 per ordinary, 0.5 for mid-range, and 1 for high-end
    
    ;; if there is no energy, there is no link to a task
    ask actors with [count my-task-links = 0 and energy > 0] [
      if any? tasks with [count my-task-links = 0] in-radius reach [
        create-task-link-to one-of tasks with [count my-task-links = 0] in-radius reach [
          set color green
        ]
      ]
      if any? my-out-links [
        let capacity ( sum [draining] of task-link-neighbors )
        if [energy] of self > capacity [
          if any? tasks with [count my-task-links = 0] in-radius reach [
            create-task-link-to one-of tasks with [count my-task-links = 0] in-radius reach [
              set color green
            ]
          ]
        ]
      ]
    ]
  ]
  
  ;; when the link is longer than reach, the connection is severed
  ask task-links [
    if link-length > reach [
      die
    ]
  ]
    
  ;; tasks that are worked on change shape to become visible
  ;; in the environment
  ask tasks [
    ifelse any? my-links with [color = green] [
      set shape "square 2"
    ] [
      set shape "square"
    ]
  ]
  
  performance

end

to ecological-rationality
  
  bounded-rationality
  
  ;; when a task cannot be performed by usual means,
  ;; the actor would make a guess to understand whether it 
  ;; can be solved using the competences they have
  
  ask actors with [any? my-task-links] [
    ;; they guess the type and urgency of the task to 
    ;; see whether they can solve it; it is a heuristic
    let guess ([competence] of self + random-float 1)
    ask my-task-links [
      let needs (1 * (1 + [urgency] of end2))
      let w-drain ([draining] of end2)
      if guess > needs [
        ask end1 [
          set energy (energy -  w-drain)
          set competence (competence * (1 + competence-increment-ratio / 3))
          set counter counter + 1
        ]
        ifelse [t-type] of end2 = "ordinary" [
          ask end1 [ set count-ord count-ord + 1 ]
        ][
          ifelse [t-type] of end2 = "mid-range" [
            ask end1 [ set count-mr count-mr + 1 ]
          ][
            ask end1 [ set count-he count-he + 1 ]
          ]
        ]
        ask end2 [die]
      ]
    ]
  ]
  
end

to biased-rationality
  
  bounded-rationality
  
  ;; this is similar to the heuristics/ecological
  ;; paradigm, but the estimate is of the target (task)
  ;; and use of resource rather than on competence

  ask actors with [any? my-task-links] [
    ;; in this case the issue is a bias; so, the right estimation
    ;; of one's own competence may allow for this actor to
    ;; then adjust competence and try again with the task, with an
    ;; enhanced way to approach it... it may still be insufficient
    let guess ([competence] of self + random-normal 0 0.5)
    if guess - [competence] of self < 0.5 [
      set competence (competence * (1 + competence-increment-ratio))
    ]
    ask my-task-links [
      let needs (1 * (1 + [urgency] of end2))
      let w-drain ([draining] of end2)
      if [competence] of end1 > needs [
        ask end1 [
          set energy (energy -  w-drain)
          ;; no need for competence increase here
          set counter counter + 1
        ]
        ifelse [t-type] of end2 = "ordinary" [
          ask end1 [ set count-ord count-ord + 1 ]
        ][
          ifelse [t-type] of end2 = "mid-range" [
            ask end1 [ set count-mr count-mr + 1 ]
          ][
            ask end1 [ set count-he count-he + 1 ]
          ]
        ]
        ask end2 [die]
      ]
    ]
  ]

  
end

to epistemic-rationality
  ;; this is a case where most difficult tasks are executed
  ;; using 'discovery' as a method of enquiry; a trial and error approach
  ;; may be more feasible here. Perhaps the serendipitous combination of 
  ;; resources leading to unexpected results.
  
  ; usual starting point
  bounded-rationality
  
  ask actors with [any? my-task-links and any? my-resource-links] [
    if count my-resource-links > 1 [
      ;; the below creates a list of types of resources available
      let r-comb [r-type] of resource-link-neighbors
      ;; this is to check whether there are type a and b among the resources
      if position "Type A" r-comb != false and position "Type B" r-comb != false [
        ;; the epistemic type modifies its competence by rearranging the two 
        ;; resources, resulting in a + or - outcome
        let fiddling competence + random-normal 0.5 0.25
        ask my-task-links [
          let needs (1 * (1 + [urgency] of end2))
          let w-drain ([draining] of end2)
          if fiddling > needs [
            ask end1 [
              set energy (energy -  w-drain)
              ;; independent of the type of task performed, there is a significant gain
              ;; here, due to cooperation with others
              set competence (competence * (1 + competence-increment-ratio))
              set counter counter + 1
            ]
            ifelse [t-type] of end2 = "ordinary" [
              ask end1 [ set count-ord count-ord + 1 ]
            ][
              ifelse [t-type] of end2 = "mid-range" [
                ask end1 [ set count-mr count-mr + 1 ]
              ][
                ask end1 [ set count-he count-he + 1 ]
              ]
            ]
            ask end2 [die]
          ]
        ]
      ]
    ]
  ]
  
  
end

to extendable-rationality
  ;; this is based on distributed cognition and my work, hence, 
  ;; I would consider social resources as well 
  
  ;; usual starting point:
  bounded-rationality
  
  ;; highly sociable people may be connected to more
  ;; people than average
  ask actors with [sociability > (0.75 * max [sociability] of actors)] [
    create-actor-links-with other actors in-radius (reach * 1.25) [
      set color yellow - 2
    ]
  ]
  ask actor-links with [color = yellow - 2] [
    if link-length <= 5 [
      ask actor-links with [link-length <= 5] [die]
    ]
  ]
  
  
  ask actors with [any? my-task-links and any? my-actor-links] [
    let add-on (mean [competence] of actor-link-neighbors) * [sociability] of self
      
    ask my-task-links [
      let needs (1 * (1 + [urgency] of end2))
      let w-drain ([draining] of end2)
      if [competence] of end1 > needs [
        ask end1 [
          set energy (energy -  w-drain)
          ;; independent of the type of task performed, there is a significant gain
          ;; here, due to cooperation with others
          set competence (competence * (1 + competence-increment-ratio))
          set counter counter + 1
        ]
        ifelse [t-type] of end2 = "ordinary" [
          ask end1 [ set count-ord count-ord + 1 ]
        ][
          ifelse [t-type] of end2 = "mid-range" [
            ask end1 [ set count-mr count-mr + 1 ]
          ][
            ask end1 [ set count-he count-he + 1 ]
          ]
        ]
        ask end2 [die]
      ]
    ]
  ]

  
end

to energy-replenish
  ask actors [
    let base energy
  ]
end

;; WORKING ON TASKS

to performance
  ;; see the notes on the info tab for details on this
  ;; There are two conditions to be met: 
  ; (a) enough competence and 
  ; (b) resource type to be sufficient for the task type
  
  ;; the line below forces action only on those actors who
  ;; are connected to a task and at least one resource
  ask actors with [any? my-task-links and any? my-resource-links] [
    let w-resource (one-of [other-end] of my-resource-links)
    
    ask my-task-links [
      if [t-type] of end2 = "ordinary" [
        let needs (1 * (1 + [urgency] of end2))
        
        ;; the first condition is that, when c > 4, given
        ;; there is a resource in use, then the task is performed
        if [competence] of end1 >= 4 [
          let w-drain ([draining] of end2)
          ask end1 [
            set energy (energy -  w-drain)
            set competence (competence * (1 + competence-increment-ratio / (ticks / 2)))
            set counter counter + 1
            set count-ord count-ord + 1
          ]
          ask end2 [die]
        ]
        if [competence] of end1 < 4 and [competence] of end1 >= 2 [
          if [r-type] of w-resource = "Type C" [
            if ([competence] of end1 + (0.5 * [potential] of w-resource) ) > needs [
              let w-drain ([draining] of end2)
              ask end1 [
                set energy (energy -  w-drain)
                set competence (competence * (1 + competence-increment-ratio / (ticks / 2)))
                set counter counter + 1
                set count-ord count-ord + 1
              ]
              ask end2 [die]
            ]
          ]
          if [r-type] of w-resource = "Type B" or [r-type] of w-resource = "Type A" [
            if ([competence] of end1 + ([potential] of w-resource) ) > needs [
              let w-drain ([draining] of end2)
              ask end1 [
                set energy (energy -  w-drain)
                set competence (competence * (1 + competence-increment-ratio / (ticks / 2)))
                set counter counter + 1
                set count-ord count-ord + 1
              ]
              ask end2 [die]
            ]
          ]
        ]
      ]
      ;; task type: mid-range
      ;; this is easier than the above, because competence needs to be
      ;; connected with the type of resource; in any case, if the connected
      ;; resource is Type A, then there is nothing to be done, independent
      ;; of how skilled/competent the actor is
      if [t-type] of end2 = "mid-range" [
        let needs (1 * (1 + [urgency] of end2))
        if [r-type] of w-resource = "Type B" [
          if ([competence] of end1 + ([potential] of w-resource) ) > needs [
            let w-drain ([draining] of end2)
            ask end1 [
              set energy (energy -  w-drain)
              set competence (competence * (1 + competence-increment-ratio / (ticks / 2)))
              set counter counter + 1
              set count-mr count-mr + 1
            ]
            ask end2 [die]
          ]
        ]
        if [r-type] of w-resource = "Type C" [
          if ([competence] of end1 + (0.5 * [potential] of w-resource) ) > needs [
            let w-drain ([draining] of end2)
            ask end1 [
              set energy (energy -  w-drain)
              set competence (competence * (1 + competence-increment-ratio / (ticks / 2)))
              set counter counter + 1
              set count-mr count-mr + 1
            ]
            ask end2 [die]
          ]
        ]
      ]
      ;; task type: high-end
      ;; this is even easier than the last one above, since
      ;; we only need a Type C and highly competent actor to perform
      ;; the task
      if [t-type] of end2 = "high-end" [
        let needs (1 * (1 + [urgency] of end2))
        if [r-type] of w-resource = "Type C" [
          if ([competence] of end1 + ([potential] of w-resource) ) > needs [
            let w-drain ([draining] of end2)
            ask end1 [
              set energy (energy -  w-drain)
              ;; I changed my mind on differentiating learning between the tasks
              ;; Now all the learning decreases over time (see the plots on the R.proj 'Rationalities'
              set competence (competence * (1 + competence-increment-ratio / (ticks / 2)))
              set counter counter + 1
              set count-he count-he + 1
            ]
            ask end2 [die]
          ]
        ]
      ]
      ;; final closing brackets
    ]
  ]
  
  ask actors [
    ifelse energy < energy-cap [
      set energy (abs energy * (1 + energy-replenish-ratio))
    ] [
      set energy energy-cap
    ]
  ]
  
end]]></code>
